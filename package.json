{
  "name": "myyoutubefilecabinet",
  "version": "1.0.0",
  "description": "Practicing backend, electron, and organize my youtube subscriptions",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "test",
    "build-renderer": "parcel build render.js --out-dir dist --out-file render.js --target browser --no-cache",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViLinh27/myYoutubeFileCabinet.git"
  },
  "keywords": [
    "javascript",
    "electron",
    "webapp",
    "app"
  ],
  "author": "Linh Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViLinh27/myYoutubeFileCabinet/issues"
  },
  "homepage": "https://github.com/ViLinh27/myYoutubeFileCabinet#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "^36.4.0",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "fuse.js": "^7.1.0"
  },
  "build": {
    "productName": "Youtube File Cabinet",
    "directories": {
