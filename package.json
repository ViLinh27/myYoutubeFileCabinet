{
  "name": "myyoutubefilecabinet",
  "version": "1.0.0",
  "description": "Practicing backend, electron, and organize my youtube subscriptions",
  "main": "main.js",
  "scripts": {
    "start": "npm run build-renderer && electron .",
    "test": "test",
    "build-renderer": "parcel build render.js --out-dir dist --out-file render.js --target browser --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViLinh27/myYoutubeFileCabinet.git"
  },
  "keywords": [
    "javascript",
    "electron",
    "webapp",
    "app"
  ],
  "author": "Linh Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViLinh27/myYoutubeFileCabinet/issues"
  },
  "homepage": "https://github.com/ViLinh27/myYoutubeFileCabinet#readme",
  "devDependencies": {
    "electron": "^36.4.0",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "electron-builder": "^26.0.12",
    "fuse.js": "^7.1.0"
  },
  "build":{
    "productName": "Youtube File Cabinet",
    "mac":{
      "icon":"./assets/icons/icon-youtubefiling.icns"
    },
    "win":{
      "icon":"./assets/icons/icon-youtubefiling.ico"
    },
    "linux":{
      "icon":"./assets/icons/icon-youtubefiling.png"
    }
  }
}
